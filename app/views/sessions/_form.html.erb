  <%= form_for(:session, url: login_path) do |f| %>
      <%= f.email_field :email, :placeholder => 'email', type:'email', class:'form-control' %><br>
      <%= f.password_field :password, :placeholder => 'password', class:'form-control' %><br>
      <%= f.submit 'Log in', type: 'submit', class:'btn btn-warning' %>
  <% end %><br>
  <nav><%= link_to t("Haven't got account?"), '#anchor',
                   {'data-toggle':  'modal', 'data-target': '#signup-modal'} %> </nav>
